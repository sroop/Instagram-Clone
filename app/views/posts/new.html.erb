<%= form_for @post, html: { multipart: true } do |f| %>
	

	<%= f.label :picture %>
	<%= f.file_field :picture %>

	<%= f.label :caption %>
	<%= f.text_field :caption %>

	<%= f.label :address %>
	<%= f.text_field :address %>

	<%= f.label :tag_name, "Tags" %>
	<%= f.text_field :tag_name %>

	<%= f.submit 'Add' %>

 <% end %>